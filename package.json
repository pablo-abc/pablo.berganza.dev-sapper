  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "NODE_ENV=production sapper export --entry '/ /404'",
    "postexport": "rm __sapper__/export/service-worker-index.html && mv __sapper__/export/404/index.html __sapper__/export/404.html",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "predeploy": "yarn export",
    "deploy": "gh-pages -d __sapper__/export/"
  },
